@using midterm.Models
@model List<JoinedOrderDetail>
@using System.Collections.Generic

<table class="table">
    <thead>
     <tr>
        <th> Id </th>
        <th> Phone  </th>
        <th> Category  </th>
        <th> Path </th>
        <th> LoadId </th>
        <th> Category </th>
        <th> Phone </th>
        <th> OrderDate </th>
        <th> DeliveryDate </th>
        <th> TotalCost </th>
        <th> Status </th>
     </tr>
    </thead>
    <tbody>
    @foreach(var item in Model)
    {
        <tr>
           <td> @item.Id </td>
           <td> @item.Phone </td>
           <td> @item.Category </td>
           <td> @item.Path </td>
            <td> @item.LoadId </td>
            <td> @item.Category </td>
            <td> @item.Phone </td>
            <td> @item.OrderDate </td>
            <td> @item.DeliveryDate </td>
            <td> @item.TotalCost </td>
            <td> @item.Status </td>
        </tr>
    }
    </tbody>
</table> 