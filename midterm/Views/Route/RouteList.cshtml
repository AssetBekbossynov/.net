@using midterm.Models
@model List<Route>
@using System.Collections.Generic

<table class="table">
    <thead>
     <tr>
       <th> Id </th>
       <th> Path  </th>
       <th> Duration  </th>
       <th> Cost  </th>
     </tr>
    </thead>
    <tbody>
    @foreach(var item in Model)
    {
        <tr>
           <td> @item.Id </td>
           <td> @item.Path </td>
           <td> @item.Duration </td>
           <td> @item.Cost </td>
        </tr>
    }
    </tbody>
</table> 